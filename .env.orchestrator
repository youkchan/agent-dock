# Lead provider
ORCHESTRATOR_PROVIDER=openai
ORCHESTRATOR_OPENAI_MODEL=gpt-5-mini
ORCHESTRATOR_INPUT_TOKENS=4000
ORCHESTRATOR_OUTPUT_TOKENS=800
ORCHESTRATOR_REASONING_EFFORT=minimal
ORCHESTRATOR_AUTO_APPROVE_FALLBACK=1
HUMAN_APPROVAL=0

# Teammate adapter
TEAMMATE_ADAPTER=subprocess
#TEAMMATE_COMMAND="bash ./codex_wrapper.sh"
# 必要なら分離
# TEAMMATE_PLAN_COMMAND="bash ./codex_wrapper.sh"
# TEAMMATE_EXECUTE_COMMAND="bash ./codex_wrapper.sh"
# 暫定: 長時間タスクでのタイムアウト回避
TEAMMATE_COMMAND_TIMEOUT=900
TEAMMATE_STREAM_LOGS=1

# codex_wrapper.sh
CODEX_BIN=codex
CODEX_SANDBOX=workspace-write
CODEX_FULL_AUTO=1
CODEX_SKIP_GIT_REPO_CHECK=1
CODEX_STREAM_LOGS=1
CODEX_STREAM_VIEW=assistant
# 必要時のみ
CODEX_MODEL=gpt-5.3-codex
